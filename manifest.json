{
  "manifest_version": 2,

  "name": "My Journal",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "My Journal",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["js/eventPage.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["*://www.google.com/*"],
      "css": ["css/main.css"],
      "js": ["js/vendor/jquery.min.js", "js/contentScript.js"]
    }
  ],

  "permissions": [

  ]
}